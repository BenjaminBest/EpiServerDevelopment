@using EpiServerDevelopment.Features.Teasers

@model DefaultTemplateViewModel

<section id="intro">
    <div class="container">
        <div class="row">
            <div class="col-md-7 col-sm-12">
                <div class="block">
                    <div class="section-title">
                        <h2>@Model.Title</h2>
                        <p>@Model.SubTitle</p>
                    </div>
                    <p>@Model.Description</p>
                </div>
            </div><!-- .col-md-7 close -->
            <div class="col-md-5 col-sm-12">
                <div class="block">
                    <img src="/Resources/img/wrapper-img.gif" alt="Img">
                </div>
            </div><!-- .col-md-5 close -->
        </div>
    </div>
</section>